#!/bin/bash
#Generated by JetBrains Toolbox 2.3.2.31487 at 2024-06-03T05:07:36.782103

declare -a intellij_args=()
declare -- wait=""

for o in "$@"; do
  if [[ "$o" = "--wait" || "$o" = "-w" ]]; then
    wait="-W"
    o="--wait"
  fi
  if [[ "$o" =~ " " ]]; then
    intellij_args+=("\"$o\"")
  else
    intellij_args+=("$o")
  fi
done

open -na "$HOME/Applications/Android Studio.app/Contents/MacOS/studio" $wait --args "${intellij_args[@]}"
