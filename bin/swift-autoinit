#!/usr/bin/env python3

import sys
import re


def find_indentation(string):
    indentation = ""
    indent_match = re.match("\s*", line)
    if indent_match:
        indentation = indent_match.group(0)
    return indentation



# [\w\d]+ represents a valid identifier
parameter_regex = r"([\w\d]+:\s*[\w\d]+)"
parameter_finder = re.compile(parameter_regex)

declarations = ""
init_lines = ""
first_line = True
indentation = ""
body_lines = ""


for line in sys.stdin:
    if first_line:
        indentation = find_indentation(line)
        first_line = False

    matches = parameter_finder.findall(line)
    for parameter in matches:
        declarations += f"{indentation}let {parameter}\n"
        parameter_name = parameter.split(":")[0]
        body_lines += f"{indentation}{indentation}self.{parameter_name} = {parameter_name}\n"

    init_lines += line

output = (declarations + "\n" + init_lines + body_lines).rstrip()
print(output)
